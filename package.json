    "buffer-equal-constant-time": "1.0.1",
    "chance": "1.0.8",
    "commander": "2.9.0",
    "compression": "1.6.2",
    "connect-multiparty": "2.0.0",
    "content-disposition": "0.5.2",
    "cookie-parser": "1.4.3",
    "debug": "2.6.7",
    "express": "4.15.2",
    "ink-docstrap": "1.3.0",
    "jsdoc": "3.4.3",
    "jsonwebtoken": "7.4.1",
    "jszip": "2.5.0",
